{
  "name": "@netin-js/crash",
  "author": "Netin Systems S.L.",
  "version": "1.0.0",
  "license": "SEE LICENSE IN LICENSE.md",
  "description": "Paquete para la gestiÃ³n de errores dentro de los sistemas de Netin Systems para TypeScript/Javascript",
  "keywords": [
    "Typescript",
    "Nodejs",
    "error",
    "javascript",
    "netin",
    "netin-js"
  ],
  "repository": {
    "type": "git",
    "url": "https://devopmytra.visualstudio.com/NetinSystems/_git/NS-Netin-Crash-TypeScript"
  },
  "engines": {
    "node": ">=12.14.0"
  },
  "main": "dist/index.js",
  "files": [
    "dist/**/*"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "all": "yarn run clean && yarn run compile && yarn run coverage && yarn run mutants",
    "clean": "rimraf \"./{coverage,.nyc_output}\" \"{stryker.log,test-results.xml,coverage,.nyc_output,dist,reports,stryker-tmp}\"",
    "compile": "tsc",
    "mocha": "mocha --full-trace --config .config/.mocharc.yml",
    "coverage": "nyc --nycrc-path .config/.nycrc.json mocha --full-trace --config .config/.mocharc.yml",
    "mutants": "stryker run .config/stryker.config.js",
    "doc": "yarn typedoc --options .config/typedoc.json --out docs/ src/",
    "getCredential": "vsts-npm-auth -config .npmrc"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": ".bin/prepare-commit-msg.cmd",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@stryker-mutator/core": "^3.3.1",
    "@stryker-mutator/javascript-mutator": "^3.3.1",
    "@stryker-mutator/mocha-framework": "^3.3.1",
    "@stryker-mutator/mocha-runner": "^3.3.1",
    "@stryker-mutator/typescript": "^3.3.1",
    "@types/mocha": "latest",
    "@types/node": "^14.0.27",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "chai": "^4.2.0",
    "commitizen": "^4.1.2",
    "cross-env": "^7.0.2",
    "cz-customizable": "^6.3.0",
    "dotenv": "^8.2.0",
    "editorconfig": "^0.15.3",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-tsdoc": "^0.2.6",
    "husky": "^4.2.5",
    "mocha": "^8.1.1",
    "mocha-junit-reporter": "^2.0.0",
    "mocha-lcov-reporter": "^1.3.0",
    "mocha-multi": "^1.1.3",
    "nyc": "^15.1.0",
    "prettier": "^2.0.5",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@types/uuid": "^8.0.0",
    "uuid": "^8.2.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog",
      "types": {
        "chore": {
          "description": "Other changes that don't modify src or test files",
          "title": "Chores"
        },
        "fix": {
          "description": "A bug fix",
          "title": "Bug Fixes"
        },
        "docs": {
          "description": "Documentation only changes",
          "title": "Documentation"
        },
        "refactor": {
          "description": "A code change that neither fixes a bug nor adds a feature",
          "title": "Code Refactoring"
        },
        "test": {
          "description": "Adding missing tests or correcting existing tests",
          "title": "Tests"
        },
        "ci": {
          "description": "Changes to our CI configuration files and scripts (example scopes: Travis, Circle, BrowserStack, SauceLabs)",
          "title": "Continuous Integrations"
        },
        "feat": {
          "description": "A new feature",
          "title": "Features"
        }
      },
      "defaultType": "refactor",
      "defaultSubject": "Work in progress commit",
      "defaultBody": "Regular commit with no final intent or minor code smells cleans"
    }
  }
}
